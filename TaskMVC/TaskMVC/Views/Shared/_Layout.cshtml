<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <script src="https://code.jquery.com/jquery-3.3.1.js"
            integrity="sha256-2Kok7MbOyxpgUVvAk/HJ2jigOSYS2auK4Pfzbm7uH60="
            crossorigin="anonymous"></script>
    <link href="~/wwwroot/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/wwwroot/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/wwwroot/CSS/StyleEdit.css" rel="stylesheet" type="text/css" />
    <script src="~/wwwroot/Scripts/modernizr-2.6.2.js"></script>
    <link href="@Url.Content("~/wwwroot/Content/Site.css")" type="text/css" rel="stylesheet"/>
    @RenderSection("scripts", required: false)
    <script src="~/wwwroot/Scripts/AddBook.js"></script>
    <title>Index</title>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>


    <script src="~/wwwroot/Scripts/jquery-1.10.2.js" type="text/javascript"></script>
    <script src="~/wwwroot/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/wwwroot/Scripts/jquery-1.10.2.min.js" type="text/javascript"></script>
    <script src="~/wwwroot/Scripts/jquery.unobtrusive-ajax.js" type="text/javascript"></script>
    <script src="~/wwwroot/Scripts/jquery.unobtrusive-ajax.js"></script>
    <script src='@Url.Content("~/wwwroot/Scripts/jquery.unobtrusive-ajax.js")'></script>
    <script src="~/wwwroot/Scripts/bootstrap.min.js"></script>
</body>
</html>